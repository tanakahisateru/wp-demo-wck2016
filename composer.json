{
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "guzzlehttp/guzzle": "^6.2",
        "aws/aws-sdk-php": "^3.18",
        "ua-parser/uap-php": "^3.4",
        "wpackagist-plugin/contact-form-7": "^4.4",
        "wpackagist-plugin/tinymce-advanced": "^4.3",
        "wpackagist-plugin/rest-api": "^2.0@beta",
        "wpackagist-theme/wck2014": "^1.3",
        "wpackagist-theme/twentythirteen": "^1.9"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "lucatume/wp-browser": "^1.14"
    },
    "config": {
        "vendor-dir": "docroot/vendor"
    },
    "extra": {
        "installer-paths": {
            "docroot/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "docroot/wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
